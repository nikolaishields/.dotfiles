ctrl + alt + super + shift + Return
  kitty

ctrl + alt + super + shift + i; {l,s,w,q}
  rft {lp,ss,sw,ks}

ctrl + alt + super + shift + d; {r,w,s}
  rofi -show {run,window,ssh}

ctrl + alt + super + shift + x; {d,p,l}
  autorandr --load {docked,portable,laptop} && dunstify -i debian-logo "sxhkd config" "Reloaded key bindings"
